@{
}
<table border='1'>
    <thead>
    <th>ID</th>
    <th> Name</th>
    <th>Email</th>
    <th>phone Number</th>
    </thead>

    @{
        <tbody>

            @foreach (var c in ViewBag.ListOfBasicInformation)
            {
                <tr>
                    <td> @c.ID </td>
                    <td> @c.Name</td>
                    <td> @c.Email</td>
                    <td> @c.PhoneNumber</td>
                </tr>
            }
        </tbody>

    }
</table>