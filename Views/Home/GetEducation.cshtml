@{
}
<table border='1'>
    <thead>
        <table border='1'>
            <thead>

            <th>ID </th>
            <th>degree</th>
            <th>school</th>
            <th>graduationYear</th>
            <th>gpa</th>
    </thead>

            @{
        <tbody>

                    @foreach (var c in ViewBag.ListOfEducation)
                    {
                <tr>
                    <td> @c.ID </td>
                    <td> @c.Degree </td>
                    <td> @c.School </td>
                    <td> @c.GraduationYear</td>
                    <td> @c.GPA</td>

                </tr>
                    }
        </tbody>

            }

</table>