@{
}
<table border='1'>
    <thead>
        <table border='1'>
            <thead>

            <th>ID </th>
            <th>jobTitle</th>
            <th>company</th>
            <th>Date Obtained</th> 
            <th>startDate</th>
            <th>endDate</th>
    </thead>

            @{
        <tbody>

                    @foreach (var c in ViewBag.ListOfCertifications)
                    {
                <tr>
                    <td> @c.ID </td>
                    <td> @c.jobTitle </td>
                    <td> @c.company </td>
                    <td> @c.responsibilities</td>
                    <td> @c.startDate </td>
                    <td> @c.endDate</td>

                </tr>
                    }
        </tbody>

            }

</table>